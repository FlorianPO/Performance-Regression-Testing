cat ${KAMELEON_WORKDIR}/pipe-cat_home_bepo_bureau_ | cat >/home/bepo/Bureau/custom/build/debian_g5k/g5k_machine && rm ${KAMELEON_WORKDIR}/pipe-cat_home_bepo_bureau_
mkdir -p $(dirname /home/bepo/Bureau/custom/build/debian_g5k/ssh_config);
cat >>/home/bepo/Bureau/custom/build/debian_g5k/ssh_config <<EOF
Host debian_g5k
  User root
  ProxyCommand ssh -F /home/bepo/Bureau/custom/build/debian_g5k/ssh_config grenoble "nc -q 1 `cat /home/bepo/Bureau/custom/build/debian_g5k/g5k_machine | awk '{print $NF}'` %p"
EOF
